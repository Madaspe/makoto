<div class="col-12 col-md-8">
  <div class="om-block">
    <div class="container">
      <div class="row">
        <%= if @live_action in [:index] do %>
        <div class="col-12 col-md-4 pr-0">
  <div class="skin_container row">
    <div class="skin">
      <canvas id="skin_container" width="230" height="400" tabindex="0" style="width: 230px; height: 400px;" skin_url={@user.skin_url} cloak_url={@user.cloak_url}></canvas>
    </div>

  </div>
</div>
<div class="col-12 col-md-8 pl-md-0 d-flex flex-column">
  <div class="row cabinet-w mt-3 d-flex align-self-stretch">

    <div class="col-6 col-md-3">
      <div class="card bg-icon-gamepad">
        <div class="card-body">
          <h5 class="card-title">Наигранно</h5>
          <p class="card-text">0ч</p>
        </div>
      </div>
    </div>
    <div class="col-6 col-md-3">
      <div class="card bg-icon-favorite">
        <div class="card-body">
          <h5 class="card-title">Любимый сервер</h5>
          <p class="card-text">Нет</p>
        </div>
      </div>
    </div>
    <div class="col-6 col-md-3">
      <div class="card bg-icon-time">
        <div class="card-body">
          <h5 class="card-title">С нами уже</h5>
          <p class="card-text"><%= round(NaiveDateTime.diff(NaiveDateTime.utc_now, @user.inserted_at) / 60 / 60 / 24)  %>д</p>
        </div>
      </div>
    </div>
    <div class="col-6 col-md-3">
      <div class="card bg-icon-like">
        <div class="card-body">
          <h5 class="card-title">Голосования в рейтингах</h5>
          <p class="card-text">0</p>
        </div>
      </div>
    </div>



  </div>

  <table class="table table-borderless table-cab-info mt-3">

    <tbody>

      <tr>
        <td>Профиль Discord</td>
        <td>
          
          
        <%= if @user.discord_info do %>
            <%= @user.discord_info.username %> (<%= link "Отвязать дискорд", to: Routes.user_cabinet_index_path(@socket, :delete, @user.username) %>)
        <% else %>
            <%= link "Привязать дискорд", to: Routes.user_session_path(@socket, :request, "discord") %>
        <% end %>

          
        </td>
      </tr>



      <tr>
        <td>Дата регистрации</td>
        <td><%= @user.inserted_at.day %>.<%= @user.inserted_at.month %>.<%= @user.inserted_at.year %> <%= @user.inserted_at.hour %>:<%= @user.inserted_at.minute %>:<%= @user.inserted_at.second %></td>
      </tr>



      <tr>
        <td>Статус</td>
        <td><%= @user.role %></td>
      </tr>

    </tbody>
  </table>


  <div class="owt mt-auto mb-4">
    <hr class="mb-2">
    
    
    <a class="btn btn-sm btn-block-om" href={Routes.user_cabinet_index_path(@socket, :list_referrals, @user.username, 1)}>Реферальная система</a>
    
    
  </div>




</div>
<% end %>
<%= if @live_action in [:up_balance] do %>
    <%= live_component MakotoWeb.UserCabinetLive.UpBalanceComponent,
    id: @user.id,
    user: @user,
    username: @user.username,
    live_action: @live_action,
    return_to: Routes.user_cabinet_index_path(@socket, :index, @user.username) %>          
<% end %>

