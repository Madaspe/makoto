<% end %>

<%= if @live_action in [:buy_status] do %>
    <%= live_component MakotoWeb.UserCabinetLive.BuyStatusComponent,
    id: @user.id,
    user: @user,
    username: @user.username,
    live_action: @live_action,
    return_to: Routes.user_cabinet_index_path(@socket, :index, @user.username) %>       
<% end %>

<%= if @live_action in [:list_referrals] do %>
    <%= live_component MakotoWeb.UserCabinetLive.ListReferralsComponent,
    id: @user.id,
    user: @user,
    username: @user.username,
    live_action: @live_action,
    return_to: Routes.user_cabinet_index_path(@socket, :index, @user.username),
    host: @host %>       
<% end %>

<%= if @live_action in [:donat_page] do %>
    <%= live_component MakotoWeb.UserCabinetLive.DonatPageComponent,
    return_to: Routes.user_cabinet_index_path(@socket, :donat_page) %>       
<% end %>

<%= if @live_action in [:rules_page] do %>
    <%= live_component MakotoWeb.UserCabinetLive.RulesPageComponent,
    return_to: Routes.user_cabinet_index_path(@socket, :rules_page) %>       
<% end %>

<%= if @live_action in [:rubins_to_coins] do %>
    <%= live_component MakotoWeb.UserCabinetLive.TradeRubinsToCoinsComponent,
    id: @user.id,
    user: @user,
    username: @user.username,
    live_action: @live_action,
    return_to: Routes.user_cabinet_index_path(@socket, :index, @user.username),
    host: @host %>       
<% end %>
</div>
<script type="text/javascript">
  let skin_container = null
  let skinViewer = null
  setInterval(() => {
    if (skinViewer !== null) {
      skinViewer.loadSkin(skin_container.getAttribute("skin_url"))
      skinViewer.loadCape(skin_container.getAttribute("cloak_url"))
    }

    if (skin_container === null) {
      skin_container = document.getElementById("skin_container")
      if (skin_container) {
          skinViewer = new skinview3d.SkinViewer({
          canvas: skin_container,
          width: 230,
          height: 400,
          skin: skin_container.getAttribute("skin_url"),
          cape: skin_container.getAttribute("cloak_url")
        });
        let control = skinview3d.createOrbitControls(skinViewer);
        control.enableRotate = true;


        skinViewer.playerObject.rotation.y = 7;
        skinViewer.animations.add(skinview3d.WalkingAnimation);
        skinViewer.animations.speed = 1;
      }
    }
  }, 500);

  
</script>

</div>
</div>
